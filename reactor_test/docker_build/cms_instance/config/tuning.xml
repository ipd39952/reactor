<?xml version="1.0" encoding="UTF-8"?>
<tuning>
  <clearCachesFrequency>100</clearCachesFrequency>
  <export>
    <acceptableFailures>0</acceptableFailures>
    <maxNumberOfExportedObjects>500</maxNumberOfExportedObjects>
    <maxParallelExports>2</maxParallelExports>
    <maxRecursionLevel>100</maxRecursionLevel>
    <timeout>60</timeout>
    <usesAllThreshold>200</usesAllThreshold>
  </export>
  <indexing>
    <maxBulkSize>200</maxBulkSize>
    <interval>60</interval>
    <optimizationInterval>86400</optimizationInterval>
    <filterTimeout>300</filterTimeout>
  </indexing>
  <jobMaxLogLength>50</jobMaxLogLength>
  <master>
    <cm>
      <minIdleSlaves>0</minIdleSlaves>
      <maxSlaves>25</maxSlaves>
      <slaveExecArguments/>
    </cm>
    <ses>
      <minIdleSlaves>0</minIdleSlaves>
      <maxSlaves>25</maxSlaves>
      <slaveExecArguments/>
    </ses>
    <te>
      <minIdleSlaves>1</minIdleSlaves>
      <maxSlaves>5</maxSlaves>
      <slaveExecArguments/>
    </te>
    <slaveExecMaxFailures>4</slaveExecMaxFailures>
    <slaveIdleTimeout>3600</slaveIdleTimeout>
    <slaveShutdownTimeout>10</slaveShutdownTimeout>
    <slaveStartupTimeout>120</slaveStartupTimeout>
  </master>
  <maxSearchResultSize>0</maxSearchResultSize>
  <minStreamingDataLength>8</minStreamingDataLength>
  <slave>
    <masterTimeout>1</masterTimeout>
    <maxNumberOfRequests>400</maxNumberOfRequests>
    <clientRequestTimeout>30</clientRequestTimeout>
    <httpRequestTimeout>120</httpRequestTimeout>
  </slave>
  <streamingTicketValidityTimeInterval>360000</streamingTicketValidityTimeInterval>
</tuning>
